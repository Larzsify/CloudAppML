ntUserId});\r\n        return data;\r\n    },\r\n});\r\nTemplate.myPresentations.events({\r\n    'submit #new-name'(e){\r\n    e.preventDefault();  \r\n    //Get value from form element\r\n    const target = e.target;\r\n    const name = target.newName.value;\r\n    Presentations.update({_id: this._id}, {$set: {name: name}})\r\n    \r\n    },\r\n    \r\n   \"click #delete\" (){\r\n       //Removes the selected presentation from collection\r\n       Presentations.remove(this._id);\r\n       \r\n   },\r\n    \"click #edit\" (){\r\n      \r\n    },\r\n});\r\n","import './User.html';\r\nMeteor.subscribe('googleDetails');\r\n","import { Template } from 'meteor/templating';\r\nimport './main.html';\r\nimport '../imports/ui/Templates/User/user.js';\r\nimport '../imports/ui/Templates/Dashboard/AddPresentation.js';\r\nimport '../imports/ui/Templates/Dashboard/Dashboard.js';","Presentations = new Mongo.Collection('presentations');\r\n"]}}]